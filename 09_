Create Table Payments_Part (
    SERNO NUMBER(10) NOT NULL,
    LTIMESTAMP TIMESTAMP(2) NOT NULL,
    CUSTOMERSERNO NUMBER(10) NOT NULL,
    CURRENCY CHAR(3) NOT NULL,
    AMOUNT NUMBER(20,10) NOT NULL,
    BASE_AMOUNT NUMBER(20,10))

PARTITION BY RANGE (LTIMESTAMP)
(PARTITION LTIMESTAMP_1 VALUES LESS THAN (TO_DATE('01/01/2019', 'DD/MM/YYYY')) TABLESPACE student_tbs,
 PARTITION LTIMESTAMP_2 VALUES LESS THAN (TO_DATE('01/01/2020', 'DD/MM/YYYY')) TABLESPACE student_tbs,
 PARTITION LTIMESTAMP_3 VALUES LESS THAN (TO_DATE('01/01/2021', 'DD/MM/YYYY')) TABLESPACE student_tbs,
 PARTITION LTIMESTAMP_4 VALUES LESS THAN (TO_DATE('01/01/2022', 'DD/MM/YYYY')) TABLESPACE student_tbs);


insert into Payments_Part ( select * from Payments);

select * from Payments_Part PARTITION (LTIMESTAMP_4);
